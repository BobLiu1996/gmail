	<description>聚合服务</description>
    <packaging>pom</packaging>

    <modules>
        <module>gmail-coupon</module>
        <module>gmail-member</module>
        <module>gmail-order</module>
        <module>gmail-product</module>
        <module>gmail-ware</module>
    </modules>
</project>
